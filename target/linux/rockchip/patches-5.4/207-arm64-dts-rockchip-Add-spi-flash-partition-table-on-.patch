From 507f6ff7b9e21c2f7250cf005cdb32614ddfb5f4 Mon Sep 17 00:00:00 2001
From: baiywt <baiywt_gj@163.com>
Date: Sun, 19 Jun 2022 13:21:22 +0800
Subject: [PATCH] arm64: dts: rockchip: Add spi flash partition table on rk3328
 Orange Pi R1 Plus

---
 .../dts/rockchip/rk3328-orangepi-r1-plus.dts  | 28 ++++++++++++++++++-
 1 file changed, 27 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus.dts b/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus.dts
index 1bb096d..cde661e 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3328-orangepi-r1-plus.dts
@@ -17,7 +17,33 @@
 	flash@0 {
 		compatible = "jedec,spi-nor";
 		reg = <0>;
-		spi-max-frequency = <10000000>;
+		spi-max-frequency = <50000000>;
+
+		partitions {
+			compatible = "fixed-partitions";
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			uboot@0 {
+				label = "uboot";
+				reg = <0x0 0x120000>;
+				read-only;
+			};
+
+			dtb@120000 {
+				label = "dtb";
+				reg = <0x120000 0x130000>;
+				read-only;
+			};
+
+			firmware@130000 {
+				compatible = "denx,uimage";
+				label = "firmware";
+				reg = <0x130000 0x1000000>;
+				read-only;
+			};
+
+		};
 	};
 };
 
-- 
2.30.2

